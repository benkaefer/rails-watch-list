<h1>MY TASKS</h1>

<ul>
  <% @lists.each do |list| %>
    <li><%= link_to list.name, list_path(list.id) %>
    <div>
      <% if list.photo.attached? %>
        <%= cl_image_tag list.photo.key, height: 300, width: 400, crop: :fill %>
      <% end %>
    </div>
    </li>
  <% end %>
</ul>
<p> <%=link_to "add a new list", new_list_path %></p>
