<h2><%= @list.name %></h2>

<%= @list.bookmarks.each do |bookmark|%>
  <img src="<%= bookmark.movie.poster_url %>">
  <p><%=bookmark.movie.title %></p>
  <p><%= bookmark.movie.rating %></p>
  <p><%= bookmark.movie.overview %></p>
<% end %>
<%=link_to "add a new bookmark", new_list_bookmark_path(@list) %>
